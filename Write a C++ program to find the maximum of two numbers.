#include<iostream>
using namespace std;
int main()
{
    int num1,num2;
    cout<<"Enter two numbers ";
    cin>>num1>>num2;
    if(num1>num2)
       cout<<num1<<"is the greatest number";
    if(num2>num1)
       cout<<num2<<"is the greatest number";
    else
       cout<<"numbers are equal";
}
